  "responseTimeOut": 60000,
  "requestTimeout": 60000,
  "testFiles": "**/*{feature,spec.js}",
  "trashAssetsBeforeRuns": true,
  "viewportHeight": 768,
  "viewportWidth": 1024,
  "projectId": "wmsdoh"
}
