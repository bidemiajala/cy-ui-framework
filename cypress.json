{
  "baseUrl": "https://admin-demo.nopcommerce.com",
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reportDir": "cypress/reports/mochareports",
      "quite": true,
      "overwrite": true,
      "html": true,
      "json": false
    }
  },
  "retries": {
